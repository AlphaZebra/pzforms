{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "peakforms/pzcountryselect",
  "version": "0.1.1",
  "title": "Peakforms Country Select",
  "category": "peakzebra",
  "icon": "smiley",
  "description": "Peakforms Country Select",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "peakforms",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "prompt": {
      "type": "string",
      "default": "Value"
    },
    "countryselect_id": {
      "type": "string",
      "help": "Unique to this page. No spaces.",
      "default": "Value"
    },
    "countryselect_name": {
      "type": "string",
      "help": "Unique to this page. No spaces."
    },
    "siteURL": {
      "type": "string"
    },
    "marginValues": {
      "type": "object",
      "default": {
        "top": "20px",
        "left": "0px",
        "right": "0px",
        "bottom": "20px"
      }
    },
    "backgroundColor": {
      "type": "string"
    }
  }
}