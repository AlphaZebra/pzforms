{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "peakforms/pzstateselect",
  "version": "0.1.1",
  "title": "Peakforms State Select",
  "category": "peakzebra",
  "icon": "smiley",
  "description": "A state select field for Peakforms",
  "example": {},
  "supports": {
    "html": false
  },
  "textdomain": "peakzebra",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "attributes": {
    "prompt": {
      "type": "string",
      "default": "Select a state"
    },
    "state_id": {
      "type": "string",
      "default": "state"
    },
    "state_name": {
      "type": "string",
      "default": "state"
    },
    "marginValues": {
      "type": "object",
      "default": {
        "top": "50px",
        "left": "10%",
        "right": "10%",
        "bottom": "50px"
      }
    },
    "paddingValues": {
      "type": "object",
      "default": {
        "top": "0px",
        "left": "0px",
        "right": "0px",
        "bottom": "0px"
      }
    },
    "backgroundColor": {
      "type": "string"
    }
  }
}